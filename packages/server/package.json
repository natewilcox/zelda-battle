  "devDependencies": {
    "@colyseus/loadtest": "^0.16.0",
    "@colyseus/testing": "^0.16.0",
    "@types/express": "^4.17.1",
    "@types/lodash": "^4.17.16",
    "@types/mocha": "^10.0.1",
    "mocha": "^10.2.0",
    "rimraf": "^5.0.0",
    "tsx": "^4.10.2",
    "typescript": "^5.0.4"
  },
  "dependencies": {
    "@colyseus/command": "^0.3.1",
    "@colyseus/monitor": "^0.16.0",
    "@colyseus/playground": "^0.16.0",
